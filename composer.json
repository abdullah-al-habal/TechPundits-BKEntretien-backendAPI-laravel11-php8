{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "andreaselia/laravel-api-to-postman": "^2.0",
        "darkaonline/l5-swagger": "^8.6",
        "filament/filament": "^3.2",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "rupadana/filament-api-service": "^3.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.64",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "nunomaduro/larastan": "^2.9",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-arch": "*",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-deprecation-rules": "1.1",
        "phpstan/phpstan-phpunit": "1.3",
        "phpunit/phpunit": "^11.0.1",
        "spaze/phpstan-disallowed-calls": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "@php artisan cache:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "check": [
            "@php ./vendor/bin/phpstan analyse",
            "@php ./vendor/bin/php-cs-fixer fix --dry-run --diff"
        ],
        "fix": [
            "@php ./vendor/bin/php-cs-fixer fix --allow-risky=yes --verbose"
        ],
        "optimize": [
            "@php artisan optimize:clear",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan view:cache",
            "composer dump-autoload -o"
        ],
        "test": [
            "vendor/bin/phpstan analyse"
        ],
        "security-check": [
            "vendor/bin/phpstan analyse"
        ],
        "pint": "./vendor/bin/pint",
        "all": [
            "composer check",
            "composer fix",
            "composer pint",
            "@php ./vendor/bin/php-cs-fixer fix --dry-run --diff",
            "composer test",
            "composer security-check",
            "composer optimize"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}